{
    "title": "ttag javascript es6 localization tool",
    "plugins": [
        "anchorjs",
        "github-buttons",
        "ga",
        "atoc"
    ],
    "pluginsConfig": {
        "github-buttons": {
            "buttons": [{
                "user": "ttag",
                "repo": "ttag",
                "type": "star",
                "size": "small"
            }]
        },
        "github-edit": {
            "repo": "ttag/ttag",
            "branch": "master"
        },
        "ga": {
            "token": "UA-90721450-1"
        },
        "edit-link": {
            "base": "https://github.com/ttag/ttag/edit/master/gitbook",
            "label": "Edit This Page"
        }
    },
    "root": "./gitbook"
}
